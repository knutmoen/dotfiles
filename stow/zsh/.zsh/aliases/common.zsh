# -----------------------------------------------------------------------------
# Common aliases (always loaded)
# -----------------------------------------------------------------------------

alias c='clear'
alias home='cd ~'

# Zsh helpers
alias zshsource='source ~/.zshrc'
alias zshconfig='code ~/.zshrc'
alias zshdir='cd ~/.zsh'
alias codezsh='code ~/.zsh'

# Dotfiles
alias dotfiles='cd ~/dotfiles'
alias confdotfiles='code ~/dotfiles'
